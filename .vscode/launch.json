{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch API (Vercel Dev)",
      "program": "${workspaceFolder}/node_modules/vercel/dist/index.js",
      "args": ["dev"],
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceFolder}",
      "autoAttachChildProcesses": true,
      "console": "integratedTerminal"
    }
  ]
}
